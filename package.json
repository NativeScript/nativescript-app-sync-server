{
  "name": "nativescript-app-sync-server",
  "description": "NativeScript AppSync server",
  "version": "1.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EddyVerbruggen/nativescript-app-sync-server.git"
  },
  "keywords": [
    "nativescript",
    "codepush",
    "code push",
    "appsync",
    "app sync"
  ],
  "author": {
    "name": "Eddy Verbruggen",
    "email": "eddyverbruggen@gmail.com",
    "url": "https://github.com/EddyVerbruggen/"
  },
  "bin": {
    "nativescript-app-sync-server": "./bin/www",
    "nativescript-app-sync-server-db": "./bin/db"
  },
  "engines": {
    "node": ">= 6.0",
    "npm": ">= 3.10.8"
  },
  "scripts": {
    "dev": "supervisor ./bin/www",
    "start": "node ./bin/www",
    "init": "node ./bin/db init",
    "upgrade": "node ./bin/db upgrade",
    "test": "make test",
    "test-win": "mocha test/api/init test/api/users test/api/auth test/api/account test/api/accessKeys test/api/apps test/api/index --recursive --timeout 15000",
    "coverage": "make coverage",
    "preinstall": "npm i -g pm2 && pm2 install pm2-logrotate"
  },
  "dependencies": {
    "aliyun-oss-upload-stream": "^1.3.0",
    "aliyun-sdk": "^1.12.4",
    "aws-sdk": "^2.1064.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "cos-nodejs-sdk-v5": "^2.11.6",
    "debug": "^4.3.3",
    "diff-match-patch": "^1.0.5",
    "express": "^4.17.2",
    "extract-zip": "^2.0.1",
    "formidable": "^2.0.1",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.2",
    "i18n": "^0.14.0",
    "jschardet": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "markdown-it": "^12.3.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "pug": "^3.0.2",
    "qiniu": "^7.4.0",
    "rand-token": "^1.0.1",
    "recursive-readdir": "^2.2.2",
    "redis": "^4.0.2",
    "request": "^2.72.0",
    "sequelize": "^6.14.1",
    "serve-favicon": "^2.5.0",
    "slash": "^4.0.0",
    "validator": "^13.7.0",
    "yargs": "^17.3.1",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.12",
    "@types/validator": "^13.7.1",
    "istanbul": "^0.4.5",
    "mocha": "^9.2.0",
    "should": "^13.2.3",
    "supertest": "^6.2.2",
    "supervisor": "^0.12.0"
