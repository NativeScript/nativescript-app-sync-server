{
  "name": "nativescript-app-sync-server",
  "description": "NativeScript AppSync server",
  "version": "1.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/EddyVerbruggen/nativescript-app-sync-server.git"
  },
  "keywords": [
    "nativescript",
    "codepush",
    "code push",
    "appsync",
    "app sync"
  ],
  "author": {
    "name": "Eddy Verbruggen",
    "email": "eddyverbruggen@gmail.com",
    "url": "https://github.com/EddyVerbruggen/"
  },
  "bin": {
    "nativescript-app-sync-server": "./bin/www",
    "nativescript-app-sync-server-db": "./bin/db"
  },
  "engines": {
    "node": ">= 14.17.0",
    "npm": ">= 6.14.6"
  },
  "scripts": {
    "dev": "ts-node-dev --rs --watch src/**/* --respawn src/app.ts",
    "build": "tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node dist/server.js",
    "eslint:fix": "eslint --fix",
    "eslint": "eslint lint",
    "test": "NODE_ENV=test ts-mocha -r tsconfig-paths/register src/**/*.test.ts --timeout 10000 --exit",
    "preinstall": "npm i -g pm2 && pm2 install pm2-logrotate",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "aws-sdk": "^2.1073.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-promise-router": "^4.1.1",
    "extract-zip": "^2.0.1",
    "formidable": "^2.0.1",
    "fp-ts": "^2.11.8",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.2",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "jschardet": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "moment": "^2.29.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "ramda": "^0.28.0",
    "rand-token": "^1.0.1",
    "recursive-readdir": "^2.2.2",
    "redis": "^4.0.3",
    "sequelize": "^6.16.1",
    "tscpaths": "0.0.9",
    "typescript": "^4.5.5",
    "validator": "^13.7.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/diff-match-patch": "^1.0.32",
    "@types/express": "^4.17.13",
    "@types/formidable": "^2.0.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.17",
    "@types/nodemailer": "^6.4.4",
    "@types/ramda": "^0.27.64",
    "@types/recursive-readdir": "^2.2.0",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@types/yazl": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "io-ts-types": "^0.5.16",
    "istanbul": "^0.4.5",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "should": "^13.2.3",
    "supertest": "^6.2.2",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0"
  },
  "files": [
    "bin",
    "config",
    "core",
    "docs",
    "docker",
    "models",
    "public",
    "routes",
    "sql",
    "views",
    "app.js",
    "README.md",
    "LICENSE"
  ]
}
